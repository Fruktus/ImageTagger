<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imagetagger.FrontController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="MenuSave" mnemonicParsing="false" text="Save" />
            <MenuItem fx:id="MenuSaveAs" mnemonicParsing="false" text="Save As…" />
            <MenuItem fx:id="MenuShowInExplorer" mnemonicParsing="false" text="Show in Explorer" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="MenuPreferences" mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="MenuQuit" mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Delete" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About ImageTagger" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <SplitPane dividerPositions="0.7" focusTraversable="false" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
        <ScrollPane focusTraversable="false" prefHeight="-1.0" prefWidth="-1.0">
          <content>
            <AnchorPane id="Content" minHeight="-1.0" minWidth="-1.0" prefHeight="545.0" prefWidth="430.0">
              <children>
                <Label fx:id="ImageLabel" alignment="CENTER" layoutX="14.0" layoutY="14.0" style="&#10;" text="No Image" textAlignment="CENTER" wrapText="false">
                           <font>
                      <Font size="18.0" fx:id="x1" />
                           </font>
                           <textFill>
                      <Color red="0.624" green="0.624" blue="0.624" fx:id="x2" />
                           </textFill></Label>
              </children>
            </AnchorPane>
          </content>
        </ScrollPane>
        <AnchorPane prefHeight="551.0" prefWidth="220.0" SplitPane.resizableWithParent="false">
          <children>
                  <VBox alignment="TOP_CENTER" prefHeight="551.0" prefWidth="266.0" spacing="5.0">
                     <children>
                  <Label alignment="CENTER" font="$x1" style="&#10;" text="Controls" textAlignment="CENTER" textFill="$x2" wrapText="false" />
                        <HBox alignment="BASELINE_RIGHT" fillHeight="false" prefHeight="26.0" prefWidth="174.0" spacing="10.0" VBox.vgrow="NEVER">
                           <children>
                              <Label fx:id="FolderSourceLabel" text="Source Folder" />
                              <Pane prefHeight="18.0" prefWidth="46.0" HBox.hgrow="ALWAYS" />
                              <Button fx:id="FolderSourceButton" mnemonicParsing="false" onMouseClicked="#chooseSrcDir" text="..." />
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <padding>
                              <Insets left="10.0" right="10.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="BASELINE_RIGHT" fillHeight="false" prefHeight="26.0" prefWidth="174.0" spacing="10.0">
                           <children>
                              <Label fx:id="FolderDestinationLabel" text="Target Folder" />
                              <Pane prefHeight="18.0" prefWidth="46.0" HBox.hgrow="ALWAYS" />
                              <Button fx:id="FolderTargetButton" mnemonicParsing="false" onMouseClicked="#chooseDstDir" text="..." />
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <padding>
                              <Insets left="10.0" right="10.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="BASELINE_RIGHT" fillHeight="false" prefHeight="26.0" prefWidth="174.0" spacing="10.0">
                           <children>
                              <RadioButton fx:id="ModeMove" mnemonicParsing="false" selected="true" text="Move">
                                 <toggleGroup>
                                    <ToggleGroup fx:id="Mode" />
                                 </toggleGroup></RadioButton>
                              <Pane prefHeight="18.0" prefWidth="46.0" HBox.hgrow="ALWAYS" />
                              <RadioButton fx:id="ModeCopy" mnemonicParsing="false" text="Copy" toggleGroup="$Mode" />
                           </children>
                           <padding>
                              <Insets left="10.0" right="10.0" />
                           </padding>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <VBox fx:id="ActionVBox" prefHeight="29.0" prefWidth="266.0" spacing="5.0">
                           <children>
                              <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="26.0" prefWidth="174.0" spacing="10.0">
                                 <children>
                                    <Button mnemonicParsing="false" prefHeight="21.0" prefWidth="32.0" />
                                    <ComboBox editable="true" prefWidth="160.0" HBox.hgrow="ALWAYS" />
                                    <Button mnemonicParsing="false" prefWidth="28.0" text="X" />
                                 </children>
                                 <padding>
                                    <Insets left="10.0" right="10.0" />
                                 </padding>
                              </HBox>
                           </children>
                        </VBox>
                        <Separator prefWidth="266.0" />
                        <Button fx:id="AddActionButton" mnemonicParsing="false" prefHeight="21.0" prefWidth="48.0" text="+">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
          </children>
        </AnchorPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label fx:id="StatusFilePath" maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Current File" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color red="0.625" green="0.625" blue="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
            <Label fx:id="StatusFilesProcessed" maxWidth="-1.0" prefHeight="16.0" prefWidth="100.0" text="Files Processed">
               <font>
                  <Font size="11.0" fx:id="x31" />
               </font>
               <textFill>
                  <Color red="0.625" green="0.625" blue="0.625" fx:id="x41" />
               </textFill>
            </Label>
        <Label fx:id="StatusTimeElapsed" font="$x3" maxWidth="-1.0" prefHeight="16.0" prefWidth="100.0" text="Time Spent" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
